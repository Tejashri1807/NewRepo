package Practice.Interview;

public class Class 
{
    public static void main( String[] args )
    {
        String a="Tejashri Balkrushna Dhobale";
        String b=" ";
        String[] c = a.split(" ");
    	for(int i=0;i<c.length;i++) { //2
    		
    		String d = c[i]; //
    		for(int j=d.length()-1;j>=0;j--) {
    			b=b+d.charAt(j);
    			
    	//System.out.print("");
    		}
    	//	System.out.print(" ");
    	}
    	System.out.print(b);
    }
}
